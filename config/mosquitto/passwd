# File password Mosquitto per CerCollettiva
# Formato: username:password_hash
# Generato con: mosquitto_passwd -c passwd username

# Utente MQTT per CerCollettiva
cercollettiva:$6$rounds=4096$salt$hash

# Utente per dispositivi IoT
iot_devices:$6$rounds=4096$salt$hash

# Utente per monitoring
monitoring:$6$rounds=4096$salt$hash
